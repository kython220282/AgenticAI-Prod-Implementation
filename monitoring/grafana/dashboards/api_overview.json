{
  "dashboard": {
    "title": "Agentic AI - API Overview",
    "tags": ["agentic-ai", "api"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(fastapi_requests_total[1m])",
            "legendFormat": "{{method}} {{path_template}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Request Duration (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(fastapi_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{path_template}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8},
        "type": "stat",
        "title": "Total Requests",
        "targets": [
          {
            "expr": "sum(fastapi_requests_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 8},
        "type": "stat",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "rate(fastapi_requests_total{status_code=~\"5..\"}[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "HTTP Status Codes",
        "targets": [
          {
            "expr": "sum by (status_code) (rate(fastapi_requests_total[1m]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Active Database Connections",
        "targets": [
          {
            "expr": "db_pool_size - db_pool_available",
            "legendFormat": "Active connections",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Redis Operations",
        "targets": [
          {
            "expr": "rate(redis_commands_total[1m])",
            "legendFormat": "{{command}}",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
