{
  "dashboard": {
    "title": "Agentic AI - System Resources",
    "tags": ["agentic-ai", "system"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "process_cpu_seconds_total",
            "legendFormat": "CPU Time",
            "refId": "A"
          },
          {
            "expr": "rate(process_cpu_seconds_total[1m]) * 100",
            "legendFormat": "CPU %",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Usage"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "RSS (MB)",
            "refId": "A"
          },
          {
            "expr": "process_virtual_memory_bytes / 1024 / 1024",
            "legendFormat": "VMS (MB)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "gauge",
        "title": "CPU Percent",
        "targets": [
          {
            "expr": "system_cpu_percent",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "gauge",
        "title": "Memory Percent",
        "targets": [
          {
            "expr": "system_memory_percent",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 75, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "gauge",
        "title": "Disk Usage",
        "targets": [
          {
            "expr": "system_disk_percent",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Open File Descriptors",
        "targets": [
          {
            "expr": "process_open_fds",
            "legendFormat": "Open FDs",
            "refId": "A"
          },
          {
            "expr": "process_max_fds",
            "legendFormat": "Max FDs",
            "refId": "B"
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Goroutines / Threads",
        "targets": [
          {
            "expr": "python_info",
            "legendFormat": "Python {{version}}",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
